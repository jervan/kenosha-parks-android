<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DetailItemView">
        <attr name="header_icon" format="integer"/>
        <attr name="title" format="string"/>
        <attr name="type" format="enum">
            <enum name="text" value="0"/>
            <enum name="video" value="1"/>
            <enum name="schedule" value="2"/>
        </attr>
    </declare-styleable>

    <attr name="sc_corner_radius" format="dimension" />
    <attr name="sc_border_width" format="dimension" />
    <attr name="sc_tint_color" format="color" />
    <attr name="sc_checked_text_color" format="color" />
    <attr name="sc_unchecked_text_color" format="color" />


</resources>